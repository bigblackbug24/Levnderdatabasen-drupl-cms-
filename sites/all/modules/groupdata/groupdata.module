	function getgroupDataByGroupId() 
	{
		$output = NULL;
		
		$groupId = arg(1);
		$dataYear = arg(2);

		$htmlData = show_group_data($groupId, $dataYear );
		
		$output = '<div class="getUrlData">'.$htmlData.'</div>';
		$output.= drupal_render(drupal_get_form('group_data_filter_form'));
		
		return $output;		
	}	
	
		
